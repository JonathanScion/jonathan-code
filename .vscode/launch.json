    "version": "0.2.0",
    "configurations": [
        {            
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/ContextFreeSQL/src/main.py",
            "python": "${workspaceFolder}/ContextFreeSQL/venv/Scripts/python.exe",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/ContextFreeSQL",
